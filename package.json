{
  "name": "@websuckit/js",
  "version": "0.0.1",
  "description": "Highly Scalable and distributed Websocket communication",
  "main": "dist/websuckit.js",
  "module": "dist/websuckit.m.js",
  "unpkg": "dist/websuckit.umd.js",
  "types": "dist/index.d.ts",
  "exports": {
    "require": "./dist/index.js",
    "default": "./dist/index.modern.js"
  },
  "amdName": "Websuckit",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && microbundle --tsconfig tsconfig.json --no-sourcemap",
    "dev": "microbundle --watch --tsconfig tsconfig.json --no-sourcemap",
    "test": "jest"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/websuckit/web-suck-it-js.git"
  },
  "keywords": [
    "websocket",
    "websocket as a service"
  ],
  "author": "websuckit",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/websuckit/web-suck-it-js/issues"
  },
  "homepage": "https://github.com/websuckit/web-suck-it-js#readme",
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-typescript": "^7.23.2",
    "@types/jest": "^29.5.6",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "microbundle": "^0.15.1",
    "node-forge": "^1.3.1"
  },
  "dependencies": {
    "isomorphic-unfetch": "^4.0.2",
    "ts-node": "^10.9.1"
  }
}
