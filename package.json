{
  "name": "@websuckit/js",
  "version": "1.0.0",
  "description": "Highly Scalable and distributed Websocket communication",
  "main": "dist/index.js",
  "module": "dist/index.m.js",
  "unpkg": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "exports": {
    "require": "./dist/index.js",
    "default": "./dist/index.modern.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && microbundle --tsconfig tsconfig.json --no-sourcemap",
    "dev": "microbundle --watch --tsconfig tsconfig.json --no-sourcemap"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bisohns/web-suck-it.git"
  },
  "keywords": [
    "websocket"
  ],
  "author": "bison corps",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bisohns/web-suck-it/issues"
  },
  "homepage": "https://github.com/bisohns/web-suck-it#readme",
  "devDependencies": {
    "microbundle": "^0.15.1"
  },
  "dependencies": {
    "node-forge": "^1.3.1"
  }
}
